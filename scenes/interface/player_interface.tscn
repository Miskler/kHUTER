func _process(_delta):
	if dos:
		dos = false
		var tween = create_tween()
		
		tween.tween_property(self, \"color:a\", (1-((PL.well_being[\"health\"])/100.0))*0.2, 0.5)
		
		tween.play()
		
		yield(tween, \"finished\")
		dos = true
