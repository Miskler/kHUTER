[sub_resource type="GDScript" id=42]
script/source = "extends Control

func reload(dat, _mode = \"news\"):
	var path = get_node(\"Menu\")
	
	for i in path.get_children():
		i.queue_free()
	
	for i in dat:
		var re := RegEx.new()
		re.compile('\\\\*\\\\*(.*?)\\\\*\\\\*')
		i[\"text\"] = re.sub(i[\"text\"], '[b]$1[/b]', true)
		re.compile('\\\\*(.*?)\\\\*')
		i[\"text\"] = re.sub(i[\"text\"], '[color=gray][i]$1[/i][color=]', true)
		re.compile('`(.*?)`')
		i[\"text\"] = re.sub(i[\"text\"], '[color=aqua][code]$1[/code][color=]', true)
		
		var node = get_node(\"*Sample\").duplicate()
		
		node.name = i[\"title\"]
		node.get_node(\"Text\").bbcode_text = i[\"text\"]
		
		path.add_child(node, true)
		node.show()
"

