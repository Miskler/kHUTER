script/source = "extends Control

func _ready():
	list_reload()

func add_element():
	var dir = Directory.new()
	if dir.open(\"user://SaveGame/Worlds\") == OK:
		dir.list_dir_begin()
		var file_name = dir.get_next()
		
		while file_name != \"\":
			var file = File.new()
			if file_name != \".\" and dir.current_is_dir() and file.file_exists(\"user://SaveGame/Worlds/\" + file_name + \"/map.khuter\"):
				var item = get_node(\"List/Box/*Sample\").duplicate()
				
				item.name = file_name
				item.name_map = file_name
				item.get_node(\"Title\").text = file_name
				
				var image = Image.new()
				var err = image.load(\"user://SaveGame/Worlds/\" + file_name + \"/screenshot.png\")
				if err == OK:
					item.get_node(\"Icon\").texture = ImageTexture.new()
					item.get_node(\"Icon\").texture.create_from_image(image, 0)
				else:
					item.get_node(\"Icon\").texture = load(\"res://textures/other/sphere_monitors/sphere_error/error1.png\")
				
				if file.open(\"user://SaveGame/Worlds/\" + file_name + \"/data.json\", File.READ) == OK:
					file.open(\"user://SaveGame/Worlds/\" + file_name + \"/data.json\", File.READ)
					var data = str2var(file.get_as_text())
					file.close()
					item.get_node(\"Description\").text = \"Версия игры \" + data[0] + \",\\nДата создания \"+ str(data[2][\"hour\"])+\":\"+str(data[2][\"minute\"])+\" - \" + str(data[2][\"day\"])+\".\"+str(data[2][\"month\"])+\".\"+str(data[2][\"year\"]) + \",\\nАвтор \" + data[1]
				else:
					item.get_node(\"Description\").text = \"Данные отцуцтвуют!\"
				
				item.show()
				get_node(\"List/Box\").add_child(item)
				item.get_node(\"Play\").catalog_name = file_name
				item.get_node(\"Play\").node = self
				item.get_node(\"Catalog\").catalog_name = file_name
				item.get_node(\"Catalog\").node = self
				item.get_node(\"Remove\").catalog_name = file_name
				item.get_node(\"Remove\").node = self
			file_name = dir.get_next()
func del_all_element():
	for i in $\"List/Box\".get_children():
		if i.name != \"*Sample\":
			i.queue_free()
func list_reload():
	del_all_element()
	add_element()


func dir_load_game(name_save):
	G.get_node(\"Load Screen\").select(\"user://SaveGame/Worlds/\" + name_save + \"/map.khuter\", true)
func dir_open(name_save):
	return OS.shell_open(str(OS.get_user_data_dir()) + \"/SaveGame/Worlds/\" + name_save)
func dir_del_game(name_save):
	$\"../Notification\".window_title = name_save
	$\"../Notification\".dialog_text = \"Вы точно хотите удалить \\\"\" + name_save + \"\\\"?\"
	$\"../Notification\".popup_centered()
# warning-ignore:return_value_discarded
	$\"../Notification\".connect(\"confirmed\", self, \"dir_del_accept\")
func dir_del_accept():
	$\"../Notification\".disconnect(\"confirmed\", self, \"dir_del_accept\")
	var path = str(OS.get_user_data_dir()) + \"/SaveGame/Worlds/\" + str($\"../Notification\".window_title)
	G.remove_recursive(path)
	list_reload()


func load_scene():
	$\"../Conductor\".popup_centered()
# warning-ignore:return_value_discarded
	$\"../Conductor\".connect(\"file_selected\", self, \"file_selected\")
# warning-ignore:return_value_discarded
	$\"../Conductor\".connect(\"hide\", self, \"hid\")
func hid(): 
	$\"../Conductor\".disconnect(\"file_selected\", self, \"file_selected\")
	$\"../Conductor\".disconnect(\"hide\", self, \"hid\")
func file_selected(path):
	G.get_node(\"Load Screen\").select(path, true)
"

