script/source = "tool
extends Node

export var visible_p = \"\"
onready var main_node = $\"../Interfaces\"

var visibles_parametr = {
	\"Settings\": Vector2(-950, 41),
	\"Saves\": Vector2(-950, 31.5),
	\"Multiplayer\": Vector2(-950, -600),
	\"Maps\": Vector2(272.5, -600),
	\"Changes\": Vector2(187.5, -600),
	\"InNet\": Vector2(300, -600),
	\"Game\": Vector2(300, -600),
}



var dos_event = [true, true]
func event():
	for i in visibles_parametr.keys():
		if i != visible_p:
			var tween = create_tween()#.set_trans(Tween.TRANS_EXPO)
			
			tween.tween_property(main_node.get_node(i), \"rect_position\", visibles_parametr[i], 0.2)
	
	if visible_p in visibles_parametr:
		if dos_event[0]:
			dos_event[0] = false
			var tween = create_tween().set_trans(Tween.TRANS_EXPO)
			
			tween.tween_property(main_node.get_node(\"Main\"), \"rect_position:y\", float(main_node.rect_size.y), 0.5)
			tween.tween_property(main_node.get_node(visible_p), \"rect_position\", main_node.rect_size/2-(main_node.get_node(visible_p).rect_pivot_offset), 0.5)
			
			yield(tween, \"finished\")
			dos_event[0] = true
	else:
		if dos_event[1]:
			dos_event[1] = false
			var tween = create_tween().set_trans(Tween.TRANS_EXPO)
			
			tween.tween_property(main_node.get_node(\"Main\"), \"rect_position:y\", float(main_node.rect_size.y-main_node.get_node(\"Main\").rect_size.y), 0.5)
			
			yield(tween, \"finished\")
			dos_event[1] = true
"

