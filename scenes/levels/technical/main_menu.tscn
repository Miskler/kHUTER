script/source = "extends Control

var translation_dictionary = {
	\"Базовое\": {
		\"boot_menu\": \"Показывать меню загрузки\",
		\"player_name\": \"Имя игрока\",
		\"bad_graphics\": \"Использовать плохую графику\",
		\"FPS\": \"ККС (ФПС/FPS)\",
		\"render_bonus\": \"Дальность отрисовки\",
		\"alternative_rendering\": \"Альтернативная отрисовка\",
	},
	\"Сеть\": {
		\"port\": \"Порт сервера\",
	},
	\"Android\": {
		\"mobile\": \"Мобильная версия\",
		\"mobile_use_all\": \"Андроид использование\",
	},
	\"Сохранения\": {
		\"auto_save\": \"Период автосохранения\",
		\"auto_on\": \"Автосохранение\",
	}
}

var descriptions_dictionary = {
	\"boot_menu\": \"Показывать меню загрузки. Нет, ты от этого ничего не потеряешь т.к. игра очень быстро обрабатывает что ей нужно. Просто ты не увидешь топовый логотип который наш художник так усердно рисовал в моём подвале :)\",
	\"bad_graphics\": \"Плохая графика... Оно тебе надо? Игра не сильно и много жрёт, да и многие карты чихать хотели какую настройку ты выставил.\",
	\"FPS\": \"Количество Кадрос в Секунду - ККС(xD). Ограничение сколько раз в секунду будет обновляться картинка в игре (если выставить 10000 вы просто будете прожигать электричество).\",
	\"port\": \"Я крайне не рекомендую менять этот порт. В игре он стандартизирован и если его поменять, то у вас могут начаться проблемы с подключением к серверам :)\",
	\"player_name\": \"Имя пользователя используется при создании сохранения, а так же в мультиплеерной игре. В названии ника нет ограничений. Хоть на японском пишите, но просто оставайтесь людьми!\",
	\"render_bonus\": \"На сколько далеко за пределами экрана будет отрисовываться мир. Чем больше значение тем больше нагрузка на видеокарту и процессор!\",
	\"mobile\": \"Мобильная версия включает в себя дополнительные кнопки на экране которые заменят вам клавиатуру, если к вашему устройству она не подключена.\",
	\"mobile_use_all\": \"Если этот параметр включен, то в мобильном интерфейсе будет показана кнопка \\\"Использовать\\\" которая (как не трудно догадаться) использует все объекты рядом с которыми вы находитесь.\\n\\nНе работает с блоками!\",
	\"auto_save\": \"Период автосохранений (в минутах).\",
	\"auto_on\": \"Влючает и выключает функцию автосохранения.\",
	\"alternative_rendering\": \"При включении этой функции отрисовка тайловой карты начинает происходить силами движка игры, что может повысить производительность.\",
}

func _ready():
	add_buttons()
	dat()

func add_buttons():
	del_buttons()
	
	for group in translation_dictionary.keys():
		var title = $\"List/Buttons/*SampleTitle\".duplicate()
		
		title.text = \"          \"+str(group)
		title.name = str(group)
		
		title.show()
		
		$List/Buttons.add_child(title)
		
		for item_key in translation_dictionary[group].keys():
			if G.game_settings[item_key] is int or G.game_settings[item_key] is float:
				var item = $\"List/Buttons/*SampleInt\".duplicate()
				
				item.name = item_key
				item.prefix = translation_dictionary[group][item_key] + \" - \"
				
				item.value = G.game_settings[item_key]
				
				item.show()
				$\"List/Buttons\".add_child(item)
			elif G.game_settings[item_key] is bool:
				var item = $\"List/Buttons/*SampleBool\".duplicate()
				
				item.name = item_key
				if translation_dictionary[group].has(item_key):
					item.text = translation_dictionary[group][item_key]
				else:
					item.text = item_key
				
				item.pressed = G.game_settings[item_key]
				
				item.show()
				$\"List/Buttons\".add_child(item)
			elif G.game_settings[item_key] is String:
				var item = $\"List/Buttons/*SampleString\".duplicate()
				
				item.name = item_key
				item.placeholder_text = translation_dictionary[group][item_key]
				
				item.text = G.game_settings[item_key]
				
				item.show()
				$\"List/Buttons\".add_child(item)
			else:
				push_error(\"Система настроек: Неизвестный тип данных - \\\"\" + str(item_key) + \"\\\": \" + str(G.game_settings[item_key]))

func del_buttons():
	for i in $List/Buttons.get_children():
		if not i.name.begins_with(\"*Sample\"):
			i.queue_free()

func set_data(title, new_param):
	G.game_settings[title] = new_param
	
	dat()
	
	var file = File.new()
	file.open(G.ways[\"settings\"], File.WRITE)
	file.store_line(to_json(G.game_settings))
	file.close()

func dat():
	Engine.set_target_fps(G.game_settings[\"FPS\"])
	$\"../../Details/kHUTER/VersionUI\".text = {true: \"Mobile\", false: \"Desktop\"}[G.game_settings[\"mobile\"]]
	
	$\"../Saves/Buttons/InFileSystem\".disabled = G.game_settings[\"mobile\"]
	$\"../Saves/List/Box/*Sample/Catalog\".disabled = G.game_settings[\"mobile\"]
	$\"../../Backgrounds/Spheres\".visible = not G.game_settings[\"bad_graphics\"]
	
	$\"../Saves\".list_reload()

func set_description(title):
	if descriptions_dictionary.has(title):
		$Description.text = descriptions_dictionary[title]
	else:
		$Description.text = \"У этого параметра нет описания\"

"

