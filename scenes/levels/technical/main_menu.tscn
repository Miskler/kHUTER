script/source = "extends Control

var data_servers = []
var data_servers_online = []


func _ready():
# warning-ignore:return_value_discarded
	get_tree().connect(\"connection_failed\", self, \"failed\")
# warning-ignore:return_value_discarded
	get_tree().connect(\"connected_to_server\", self, \"go\")
	
# warning-ignore:return_value_discarded
	$Buttons/Reload.connect(\"pressed\", self, \"reload\")
# warning-ignore:return_value_discarded
	$Buttons/MyServers.connect(\"pressed\", self, \"reload\")
# warning-ignore:return_value_discarded
	$Buttons/Connect.connect(\"pressed\", self, \"show_menu_connect\")
# warning-ignore:return_value_discarded
	$Buttons/Add.connect(\"pressed\", self, \"show_menu_add\")
# warning-ignore:return_value_discarded
	$Add/AddMenu/AddServer.connect(\"pressed\", self, \"add\")
	
	load_local_data()
	reload()


func reload():
	delete_items()
	save_local_data()
	if $Buttons/MyServers.pressed == false:
		get_node(\"/root/Menu\").request(\"servers\")
	else:
		load_local_data()
		add_items(data_servers)


func go():
	G.get_node(\"Load Screen\").select(\"res://scenes/levels/technical/root_game.tscn\")

func failed():
	get_tree().network_peer = null
	$Pass.hide()
	
	$\"../Notification\".window_title = \"Ошибка подключения!\"
	$\"../Notification\".dialog_text = \"Не возможно подключится по\\nуказаному адресу!\"
	
	$\"../Notification\".popup_centered()


func add():
	var name_server = $Add/AddMenu/NameServer.text
	var ip = $Add/AddMenu/IpServer.text
	var port = $Add/AddMenu/PortServer.value
	
	$Add.hide()
	
	var dat = {
		\"ip\": ip,
		\"port\": port,
		\"name\": name_server
	}
	
	data_servers.append(dat)
	reload()

func show_menu_connect():
	$Connect.show()
	$Connect/AddMenu/IpServer.text = \"\"
	$Connect/AddMenu/PortServer.value = G.game_settings[\"port\"] 

func show_menu_add():
	$Add/AddMenu/IpServer.text = \"\"
	$Add/AddMenu/NameServer.text = \"\"
	$Add/AddMenu/PortServer.value = G.game_settings[\"port\"]
	
	$Add.show()

func connect_to_adress(ip, port):
	var peer = NetworkedMultiplayerENet.new()
	
	peer.create_client(ip, int(port))
	get_tree().network_peer = peer
	$Pass.show()

func remove_adress(id):
	for i in data_servers:
		if i.has(\"name\") and i[\"name\"] == id:
			data_servers.erase(i)
			reload()
			save_local_data()
			break

func save_adress(id):
	for i in data_servers_online:
		if i.has(\"name\") and i[\"name\"] == id:
			data_servers.append(i)
			reload()
			save_local_data()
			break


func load_local_data():
	var file = File.new()
	if not file.file_exists(G.ways[\"data_servers\"]):
		return
	file.open(G.ways[\"data_servers\"], File.READ)
	data_servers = parse_json(file.get_as_text())
	file.close()
func save_local_data():
	var file = File.new()
	file.open(G.ways[\"data_servers\"], File.WRITE)
	file.store_line(to_json(data_servers))
	file.close()

func add_items(servers, online_mode = false):
	if servers is Array and servers.size() > 0:
		$\"No Servers\".hide()
		for i in servers:
			var node = $\"List/Box/*Sample\".duplicate()
			
			if i.has(\"ip\"):
				node.get_node(\"IP\").text = i[\"ip\"]
			if i.has(\"port\") and i[\"port\"] != null:
				node.get_node(\"PORT\").text = str(i[\"port\"])
			if i.has(\"name\"):
				node.get_node(\"Title\").text = i[\"name\"]
			if i.has(\"players\") and i.has(\"players_live\"):
				node.get_node(\"Description\").text = \"\"
			if i.has(\"players\"):
				node.get_node(\"Description\").text = \"Игроки: \" + str(i[\"players\"])
			if i.has(\"players_live\"):
				node.get_node(\"Description\").text = \"Живые игроки: \" + str(i[\"players_live\"])
			
			if online_mode == true:
				node.get_node(\"Remove\").hide()
			else:
				node.get_node(\"Save\").hide()
			
			node.show()
			$List/Box.add_child(node, true)
	else:
		$\"No Servers\".show()
func delete_items():
	for i in $List/Box.get_children():
		if i.name != \"*Sample\":
			i.queue_free()
"

