script/source = "extends Control

func _ready():
	AchievementsClient.unlock(\"CgkIv-Ln87AIEAIQAQ\") #Начало работы
	
	$Details/Version.text = \"Версия игры: \" + G.game_settings[\"version\"]
	#$Animations.play_backwards(\"BlackScreen\")
	
# warning-ignore:return_value_discarded
	get_node(\"HTTPRequest\").connect(\"request_completed\", self, \"complite_load_data\")
	request(\"all\")

func request(dat = \"all\"):
	var HTTP = get_node(\"HTTPRequest\")
	
	var error = HTTP.request(\"http://\"+G.ways[\"server\"]+\":\"+str(G.game_settings[\"port\"])+\"/\"+dat)
	if error != OK:
		push_warning(\"В HTTP-запросе произошла ошибка. (http://\"+G.ways[\"server\"]+\":\"+str(G.game_settings[\"port\"])+\"/)\")
	else:
		print(\"Запрос \\\"\"+str(dat)+\"\\\" отправлен корректно, ожидание ответа сервера... (http://\"+G.ways[\"server\"]+\":\"+str(G.game_settings[\"port\"])+\"/)\")
func complite_load_data(_result, _response_code, _headers, body):
	var response = parse_json(body.get_string_from_utf8())
	
	if response == null:
		push_warning(\"Сервер прислал битый ответ!\")
		return
	
	var dat = response[\"data\"]
	match response[\"request\"]:
		\"all\":
			$\"Interfaces/Changes\".reload(dat[0], \"news\")
			
			$\"Interfaces/Maps\".reload(dat[1])
			
			$\"Interfaces/Multiplayer\".data_servers_online = dat[2]
			$\"Interfaces/Multiplayer\".delete_items()
			$\"Interfaces/Multiplayer\".add_items(dat[2], true)
			
			if dat.size() >= 4 and str(dat[3]) != str(G.game_settings[\"version\"]):
				$Interfaces/Notification.window_title = \"Обновитесь!\"
				$Interfaces/Notification.dialog_text = \"Текущая версия: \"+G.game_settings[\"version\"]+\"\\nРекомендуемая сервером версия: \"+dat[3]
				$Interfaces/Notification.popup_centered()
			elif str(G.game_settings[\"version\"]) != str(G.game_settings[\"old_version\"]):
				$Interfaces/Notification.window_title = \"Спасибо что остаётесь с нами!\"
				$Interfaces/Notification.dialog_text = \"Текущая версия: \"+G.game_settings[\"version\"]+\"\\nПредидущая: \"+G.game_settings[\"old_version\"]
				$Interfaces/Notification.popup_centered()
				
				$Interfaces/Settings.set_data(\"old_version\", str(G.game_settings[\"version\"]))
		\"servers\":
			$\"Interfaces/Multiplayer\".data_servers_online = dat[0]
			$\"Interfaces/Multiplayer\".delete_items()
			$\"Interfaces/Multiplayer\".add_items(dat[0], true)
		\"maps\":
			$\"Interfaces/Maps\".reload(dat[0])
		\"news\":
			$\"Interfaces/Changes\".reload(dat[0], \"news\")
		\"GET_map\":
			download(dat[0], dat[1], dat[2])
		\"GET_map_error\":
			$\"Interfaces/Pass\".hide()
			
			$Interfaces/Notification.popup_centered()
			$Interfaces/Notification.dialog_text = \"Карта на сервере не найдена!\"

func download(name_map, param, map):
	$\"Interfaces/Pass\".hide()
	
	var dirSAV = Directory.new()
	var path = \"user://SaveGame/Worlds/\" + str(name_map)
	if !dirSAV.dir_exists(path):
		dirSAV.make_dir_recursive(path)
	
	var file = File.new()
	file.open(path + \"/map.huter\", File.WRITE)
	file.store_line(map)
	file.close()
	
	file = File.new()
	file.open(path + \"/data.json\", File.WRITE)
	file.store_line(to_json(param))
	file.close()
	
	$\"Animations\".play(\"MapsToSaves\")
	$\"Interfaces/Saves\".list_reload()
"

[sub_resource type="Shader" id=4]
code = "shader_type canvas_item;
render_mode unshaded;

uniform int iterations = 17;
uniform float formuparam = 0.6;

uniform int volsteps = 1;
uniform float stepsize = 0.4;

uniform float zoom = 0.1;
uniform float tile = 0.46;
uniform float speed = 0.002;

uniform float brightness = 0.001;
uniform float darkmatter = 0.003;
uniform float distfading = 0.650;
uniform float saturation = 0.450;

uniform vec2 iResolution = vec2(19, 1);
uniform vec2 iMouse = vec2(0,0);

float SCurve (float value) {

    if (value < 0.5)
    {
        return value * value * value * value * value * 10.0; 
    }
    
    value -= 1.0;
    
    return value * value * value * value * value * 10.0 + 1.0;
}

void fragment()
{
	//get coords and direction
	vec2 uv=FRAGCOORD.xy/iResolution.xy-0.5;
	uv.y*=iResolution.y/iResolution.x;
	vec3 dir=vec3(uv*zoom,1.0);
	float time=TIME*speed+.25;

	//mouse rotation
	float a1=0.5+iMouse.x/iResolution.x*2.;
	float a2=0.8+iMouse.y/iResolution.y*2.;
	mat2 rot1=mat2(vec2(cos(a1),sin(a1)),vec2(-sin(a1),cos(a1)));
	mat2 rot2=mat2(vec2(cos(a2),sin(a2)),vec2(-sin(a2),cos(a2)));
	dir.xy*=rot1;
	dir.xz*=rot2;
	vec3 from=vec3(1.0,0.5,0.5);
	from-=vec3(0.0,time,0.0);
	from.xz*=rot1;
	from.xy*=rot2;
	
	//volumetric rendering
	float s=0.1,fade=1.;
	vec3 v=vec3(0.);
	for (int r=0; r<volsteps; r++) {
		vec3 p=from+s*dir*0.5;
		p = abs(vec3(tile)-mod(p,vec3(tile*2.))); // tiling fold
		float pa,a=pa=0.;
		for (int i=0; i<iterations; i++) { 
			p=abs(p)/dot(p,p)-formuparam; // the magic formula
			a+=abs(length(p)-pa); // absolute sum of average change
			pa=length(p);
		}
		float dm=max(0.,darkmatter-a*a*.001); //dark matter
		a = pow(a, 2.3); // add contrast
		if (r>6) fade*=1.-dm; // dark matter, don't render near
		//v+=vec3(dm,dm*.5,0.);
		v+=fade;
		v+=vec3(s,s*s,s*s*s*s)*a*brightness*fade; // coloring based on distance
		fade*=distfading; // distance fading
		s+=stepsize;
	}
    
	v=mix(vec3(length(v)),v,saturation); //color adjust
    
    vec4 C = vec4(v*.01,1.);
    
     	C.r = pow(C.r, 0.35); 
 	 	C.g = pow(C.g, 0.36); 
 	 	C.b = pow(C.b, 0.38); 
 	
    vec4 L = C;   	
    
    COLOR.r = mix(L.r, SCurve(C.r), 0.7); 
    COLOR.g = mix(L.g, SCurve(C.g), 1.0); 
    COLOR.b = mix(L.b, SCurve(C.b), 0.2);     	
	
}"

